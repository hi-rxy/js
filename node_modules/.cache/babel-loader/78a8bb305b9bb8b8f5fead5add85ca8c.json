,MAAf,CAAN;AACAiB,QAAAA,MAAM,CAAC,WAAD,EAAchB,KAAd,CAAN;AACAgB,QAAAA,MAAM,CAAC,kBAAD,EAAqBf,YAArB,CAAN;AACAmB,QAAAA,OAAO,CAACK,IAAD,CAAP;AACH,OAfD,EAeGI,KAfH,CAeS,UAAAC,KAAK,EAAI;AACdT,QAAAA,MAAM,CAACS,KAAD,CAAN;AACH,OAjBD;AAkBH,KAnBM,CAAP;AAoBH,GA3CW;AA6CZ;AACA1C,EAAAA,MA9CY,yBA8CuB;AAAA,QAA1B4B,MAA0B,SAA1BA,MAA0B;AAAA,QAAlBb,KAAkB,SAAlBA,KAAkB;AAAA,QAAZ4B,QAAY,SAAZA,QAAY;AAC/B,WAAO,IAAIZ,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACpCjC,MAAAA,OAAM,CAACe,KAAK,CAACN,KAAP,CAAN,CAAoB0B,IAApB,CAAyB,YAAM;AAC3B/B,QAAAA,WAAW,GADgB,CACb;;AACdE,QAAAA,WAAW;AACXsB,QAAAA,MAAM,CAAC,aAAD,CAAN;AACAA,QAAAA,MAAM,CAAC,WAAD,EAAc,EAAd,CAAN;AACAA,QAAAA,MAAM,CAAC,WAAD,EAAc,EAAd,CAAN;AACAe,QAAAA,QAAQ,CAAC,sBAAD,EAAyB,IAAzB,EAA+B;AAAEC,UAAAA,IAAI,EAAE;AAAR,SAA/B,CAAR;AACAZ,QAAAA,OAAO;AACV,OARD,EAQGS,KARH,CAQS,UAAAC,KAAK,EAAI;AACdT,QAAAA,MAAM,CAACS,KAAD,CAAN;AACH,OAVD;AAWH,KAZM,CAAP;AAaH,GA5DW;AA8DZ;AACAG,EAAAA,UA/DY,6BA+DW;AAAA,QAAVjB,MAAU,SAAVA,MAAU;AACnB,WAAO,IAAIG,OAAJ,CAAY,UAAAC,OAAO,EAAI;AAC1B5B,MAAAA,WAAW,GADe,CACZ;;AACdwB,MAAAA,MAAM,CAAC,aAAD,CAAN;AACAA,MAAAA,MAAM,CAAC,WAAD,EAAc,EAAd,CAAN;AACAI,MAAAA,OAAO;AACV,KALM,CAAP;AAMH,GAtEW;AAwEZ;AACMc,EAAAA,WAzEM,8BAyE4BC,IAzE5B,EAyEkC;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAA1BnB,cAAAA,MAA0B,SAA1BA,MAA0B,EAAlBe,QAAkB,SAAlBA,QAAkB;AACpClC,cAAAA,KADoC,GAC5BsC,IAAI,GAAG,QADqB;AAG1CnB,cAAAA,MAAM,CAAC,WAAD,EAAcnB,KAAd,CAAN;AACAN,cAAAA,QAAQ,CAACM,KAAD,CAAR;AAJ0C;AAAA,qBAMlBkC,QAAQ,CAAC,SAAD,CANU;;AAAA;AAAA;AAMlC7B,cAAAA,KANkC,mBAMlCA,KANkC;AAQ1CR,cAAAA,WAAW,GAR+B,CAU1C;;AAV0C;AAAA,qBAWfqC,QAAQ,CAAC,2BAAD,EAA8B7B,KAA9B,EAAqC;AAAE8B,gBAAAA,IAAI,EAAE;AAAR,eAArC,CAXO;;AAAA;AAWpCI,cAAAA,YAXoC;AAY1C;AACA3C,cAAAA,MAAM,CAAC4C,SAAP,CAAiBD,YAAjB,EAb0C,CAe1C;;AACAL,cAAAA,QAAQ,CAAC,sBAAD,EAAyB,IAAzB,EAA+B;AAAEC,gBAAAA,IAAI,EAAE;AAAR,eAA/B,CAAR;;AAhB0C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiB7C;AA1FW,CAAhB;AA6FA,eAAe;AACXM,EAAAA,UAAU,EAAE,IADD;AAEXnC,EAAAA,KAAK,EAALA,KAFW;AAGXC,EAAAA,SAAS,EAATA,SAHW;AAIXU,EAAAA,OAAO,EAAPA;AAJW,CAAf","sourcesContent":["import { login, logout, getInfo } from '@/api/user'\nimport { getToken, setToken, removeToken } from '@/utils/auth'\nimport router, { resetRouter } from '@/router'\nimport { Message } from 'element-ui'\n\nconst getDefaultState = () => {\n    return {\n        token: getToken(),\n        name: '',\n        avatar: '',\n        email: '',\n        introduction: '',\n        roles: []\n    }\n}\n\nconst state = getDefaultState()\n\nconst mutations = {\n    RESET_STATE: (state) => {\n        Object.assign(state, getDefaultState())\n    },\n    SET_TOKEN: (state, token) => {\n        state.token = token\n    },\n    SET_NAME: (state, name) => {\n        state.name = name\n    },\n    SET_EMAIL: (state, email) => {\n        state.email = email\n    },\n    SET_AVATAR: (state, avatar) => {\n        state.avatar = avatar\n    },\n    SET_INTRODUCTION: (state, introduction) => {\n        state.introduction = introduction\n    },\n    SET_ROLES: (state, roles) => {\n        state.roles = roles\n    }\n}\n\nconst actions = {\n    // user login\n    login({ commit }, userInfo) {\n        const { username, password } = userInfo\n        return new Promise((resolve, reject) => {\n            login({ username: username.trim(), password: password }).then(response => {\n                const { data } = response\n                Message({\n                    message: '登录成功',\n                    type: 'success',\n                    duration: 2 * 1000\n                })\n                commit('SET_TOKEN', data.token)\n                setToken(data.token)//写入cookie记住用户登录状态\n                resolve()\n            }).catch(error => {\n                reject(error)\n            })\n        })\n    },\n\n    // get user info\n    getInfo({ commit, state }) {\n        return new Promise((resolve, reject) => {\n            getInfo(state.token).then(response => {\n                const { data } = response\n\n                if (!data) {\n                    return reject('Verification failed, please Login again.')\n                }\n                // console.log('getInfo：', response)\n                const { roles, name, avatar, email , introduction } = data\n\n                commit('SET_ROLES', roles)\n                commit('SET_NAME', name)\n                commit('SET_AVATAR', avatar)\n                commit('SET_EMAIL', email)\n                commit('SET_INTRODUCTION', introduction)\n                resolve(data)\n            }).catch(error => {\n                reject(error)\n            })\n        })\n    },\n\n    // user logout\n    logout({ commit, state,dispatch }) {\n        return new Promise((resolve, reject) => {\n            logout(state.token).then(() => {\n                removeToken() // must remove  token  first\n                resetRouter()\n                commit('RESET_STATE')\n                commit('SET_TOKEN', '')\n                commit('SET_ROLES', [])\n                dispatch('tagsView/delAllViews', null, { root: true })\n                resolve()\n            }).catch(error => {\n                reject(error)\n            })\n        })\n    },\n\n    // remove token\n    resetToken({ commit }) {\n        return new Promise(resolve => {\n            removeToken() // must remove  token  first\n            commit('RESET_STATE')\n            commit('SET_TOKEN', '')\n            resolve()\n        })\n    },\n\n    // dynamically modify permissions\n    async changeRoles({ commit, dispatch }, role) {\n        const token = role + '-token'\n\n        commit('SET_TOKEN', token)\n        setToken(token)\n\n        const { roles } = await dispatch('getInfo')\n\n        resetRouter()\n\n        // generate accessible routes map based on roles\n        const accessRoutes = await dispatch('permission/generateRoutes', roles, { root: true })\n        // dynamically add accessible routes\n        router.addRoutes(accessRoutes)\n\n        // reset visited views and cached views\n        dispatch('tagsView/delAllViews', null, { root: true })\n    }\n}\n\nexport default {\n    namespaced: true,\n    state,\n    mutations,\n    actions\n}\n\n"]}]}